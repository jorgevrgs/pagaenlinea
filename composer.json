{
  "name": "jorgevrgs/pagaenlinea",
  "description": "Paga En Linea RBM Payment Module For PrestaShop 1.7",
  "version": "2.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Jorge Vargas",
      "email": "jorgevargasla@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "App\\SocialLogin\\": "src/"
    },
    "classmap": [
      "classes/",
      "libraries/",
      "models/"
    ],
    "exclude-from-classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false
  },
  "type": "prestashop-module",
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  }
}
